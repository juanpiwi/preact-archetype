{
  "extends": "eslint-config-rackt",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "mocha": true,
    "node": true
  },
  "rules": {
    "no-unused-vars": ["error", { "varsIgnorePattern": "h" }],
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "react/jsx-no-undef": 2
  },
  "plugins": [
    "react"
  ]
}
